{
  "cli": {
    "version": ">= 5.9.1",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
    },
    "development-simulator": {
      "channel": "development-simulator",
      "developmentClient": true,
      "distribution": "internal",
      "ios": {
        "simulator": true
      }
    },
    "preview": {
      "distribution": "internal",
      "channel": "preview"
    },
    "production": {
      "ios": {
        "image": "latest"
      },
      "autoIncrement": true,
    }
  },
  "submit": {
    "production": {}
  }
}
